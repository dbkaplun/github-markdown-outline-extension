{
  "manifest_version": 2,
  "name": "Github Markdown Outline Extension",
  "description": "Automatically add an outline to the top of Markdown documents in Github",
  "version": "1.0.0",
  "homepage_url": "https://github.com/dbkaplun/github-markdown-outline-extension",
  "author": "Dan Kaplun <dbkaplun@gmail.com>",
  "icons": {},
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "js": ["index.js"],
    "css": ["index.css"]
  }],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage"
  ]
}
